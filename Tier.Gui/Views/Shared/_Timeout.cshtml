<div tabindex="-2" class="modal fade bs-example-modal-smSTO" role="dialog" aria-hidden="true" style="display: none" id="modalTimeout" data-backdrop="static">
    <div class="modal-dialog" style=" max-width: 600px;">
        <div class="modal-content">
            <form action="" method="post" id="frmSesionTimeout">
                <div class="modal-header">
                    <h4 class="modal-title">Alerta de sesión</h4>
                </div>
