                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary" data-dismiss="modal" onclick="window.SessionTimeout.sendKeepAlive();">Reanudar</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
