<div tabindex="-1" class="modal fade bs-example-modal-sm1" role="dialog" aria-hidden="true" style="display: none;">
    <div class="modal-dialog modal-sm" style="width: 320px;">
        <div class="modal-content">
            <form action="" method="post" id="frmGesationVentanas">
                @Html.Hidden("hfdIdVentana")
                @Html.Hidden("hfdActivoVentana")
                @Html.Hidden("hfdIdTroquelVentana")
                <div class="modal-header">
                    <button class="close" aria-label="Close" type="button" data-dismiss="modal">
                        <span aria-hidden="true">×</span>
                    </button>
                    <h4 class="modal-title" id="myModalLabel2">Datos ventana</h4>
                </div>
                <div class="modal-body">
                    <div class="form-group">
                        <div class="col-md-12 col-sm-12 col-xs-12 form-group has-feedback">
                            @Html.TextBox("txtLargo", null, new { @class = "form-control has-feedback-left", placeholder = "Largo", data_val = "true", data_val_required = "Dato requerido", data_val_range = "El valor debe estar entre 0 y 1000", data_val_range_min = "0", data_val_range_max = "1000" })
                            @Html.ValidationMessage("txtLargo")
                            <span class="fa fa-superscript form-control-feedback left" aria-hidden="true"></span>
                        </div>
                        <div class="col-md-12 col-sm-12 col-xs-12 form-group has-feedback">
                            @Html.TextBox("txtAlto", null, new { @class = "form-control has-feedback-left", placeholder = "Alto", data_val = "true", data_val_required = "Dato requerido", data_val_range = "El valor debe estar entre 0 y 1000", data_val_range_min = "0", data_val_range_max = "1000" })
                            @Html.ValidationMessage("txtAlto")
                            <span class="fa fa-superscript form-control-feedback left" aria-hidden="true"></span>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <div class="modal-footer">
                        <button class="btn btn-default" type="button" data-dismiss="modal">Cerrar</button>
                        <button class="btn btn-primary" type="button" onclick="Produccion.AgregarVentana()">Agregar</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
